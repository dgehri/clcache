{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo build"
		},
		{
			"label": "cargo build --release",
			"type": "cargo",
			"command": "build",
			"args": [
				"--release"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$rustc"
			]
		}
	]
}